{
  "name": "areaweb/laravel-package",
  "version": "1.0.0",
  "description": "Example Laravel package for AreaWeb",
  "autoload": {
    "psr-4": {
      "AreaWeb\\LaravelPackage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AreaWeb\\LaravelPackage\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Mikhail Protasevich",
      "email": "hello@areaweb.su"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AreaWeb\\LaravelPackage\\Providers\\LaravelPackageServiceProvider"
      ]
    }
  }
}
