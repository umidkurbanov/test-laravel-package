{
  "name": "umid/laravel-resova-api",
  "description": "Laravel package of Resova API PHP client",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "resova",
    "api",
    "client",
    "facade"
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": ">=5.4"
  },
  "require-dev": {

  },
  "license": "MIT",
  "authors": [
    {
      "name": "Paul Rock",
      "email": "paul@drteam.rocks"
    }
  ],
  "autoload": {
    "psr-4": {
      "ResovaApi\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ResovaApi\\Tests\\": "./tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "UmidPackage\\UmidPackageServiceProvider"
      ],
      "aliases": {
        "UmidPackage": "UmidPackage\\UmidPackageFacade"
      }
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
